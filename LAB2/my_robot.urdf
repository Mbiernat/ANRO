<?xml version="1.0"?>
<robot name="myfirst">
<link name="base_link">
	<visual>
	    <origin xyz="0 0 0.05" rpy="0 0 0"/>
		<geometry>
			<box size="10 0.1 0.1"/>
		</geometry>
	</visual>	
</link>

<link name="part1">
<visual>
  <origin xyz="0 0 0.55" rpy="90 90 0"/>
    <geometry>
       <box size="10 0.1 0.1"/>
    </geometry>
</visual>
</link>

<link name="part2">
<visual>
  <origin xyz="0 0 0" rpy="0 90 90"/>
    <geometry>
      <box size="10 0.1 0.1"/>
    </geometry>
</visual>
</link>

<joint name="base_to_part1" type="fixed">
    <parent link="base_link"/>
    <child link="part1"/>
</joint>

  <joint name="joint_z" type="prismatic">
    <parent link="part1"/>
    <child link="part2"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-10" upper="10" velocity="0.5"/>
  </joint>


</robot>
